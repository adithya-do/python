# -*- mode: python ; coding: utf-8 -*-
from PyInstaller.utils.hooks import collect_submodules, collect_data_files

hidden = ['oracledb'] + collect_submodules('oracledb')
datas = collect_data_files('oracledb')

block_cipher = None

a = Analysis(
    ['oracle_db_health_gui_emojis_v6.py'],
    pathex=['.'],
    binaries=[],
    datas=datas,
    hiddenimports=hidden,
    noarchive=False
)
pyz = PYZ(a.pure, a.zipped_data, cipher=block_cipher)
exe = EXE(
    pyz,
    a.scripts,
    a.binaries,
    a.zipfiles,
    a.datas,
    name='OracleDBHealthV6',
    console=False,  # set True if you want a debug console
)
